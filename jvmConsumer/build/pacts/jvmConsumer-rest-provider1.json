{
    "provider": {
        "name": "provider1"
    },
    "consumer": {
        "name": "jvmConsumer-rest"
    },
    "interactions": [
        {
            "description": "a request for Greetings",
            "request": {
                "method": "GET",
                "path": "/greetings/shane"
            },
            "response": {
                "status": 200,
                "headers": {
                    "Content-Type": "application/json;charset=UTF-8"
                },
                "body": {
                    "name": "shane",
                    "salutation": "Mr."
                }
            }
        }
    ],
    "metadata": {
        "pact-specification": {
            "version": "3.0.0"
        },
        "pact-jvm": {
            "version": "3.5.12"
        }
    }
}