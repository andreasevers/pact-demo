{
    "consumer": {
        "name": "some-test-consumer"
    },
    "provider": {
        "name": "an unknown provider"
    },
    "interactions": [
        {
            "description": "a test request",
            "request": {
                "method": "get",
                "path": "/weather",
                "query": ""
            },
            "response": {
                "status": 200,
                "headers" : {
                    "Content-type": {
                      "json_class": "Pact::Term",
                      "data": {
                        "generate": "text/plain",
                        "matcher": {
                          "json_class": "Regexp",
                          "o": 0,
                          "s": "text"
                        }
                      }
                    }
                },
                "body": {
                    "message" : {
                      "json_class": "Pact::Term",
                      "data": {
                        "generate": "rainy",
                        "matcher": {
                          "json_class": "Regexp",
                          "o": 0,
                          "s": "rain"
                        }
                      }
                    }
                }
            },
            "provider_state": "the weather is sunny"
        }
    ]
}